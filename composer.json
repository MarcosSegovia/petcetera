{
    "name": "marcossegovia/petcetera",
    "type": "library",
    "description": "Petcetera is a php library to provide output raw concatenation of elements with commas and terminators of different languages.",
    "homepage": "https://www.marcossegovia.com",
    "keywords": [
        "php", "util", "locale", "concatenation", "multilingual"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Segovia",
            "email": "velozmarkdrea@gmail.com",
            "homepage": "http://www.marcossegovia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "MarcosSegovia\\Petcetera\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcosSegovia\\PetceteraTests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
